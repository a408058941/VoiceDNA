            android:name="design_width_in_dp"
            android:value="360" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="640" />
        <meta-data android:name="com.amap.api.v2.apikey"
            android:value="11c7f690976c31d7c026b623e20e6d12">
        </meta-data>
    </application>
</manifest>
